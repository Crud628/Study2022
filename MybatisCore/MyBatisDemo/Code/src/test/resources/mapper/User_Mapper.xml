<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lan.mybatis.test.dao.IUserDao">

    <select id="queryUserInfoById" parameterType="java.lang.Long" resultType="com.lan.mybatis.po.User">
        SELECT id, userId, userName, userHead, createTime
        FROM user
        where id = #{id}
    </select>

    <select id="queryUserList" parameterType="com.lan.mybatis.test.po.User" resultType="com.lan.mybatis.po.User">
        SELECT id, userId, userName, userHead, createTime, updateTime
        FROM user
        where userId = #{userId}
    </select>

</mapper>